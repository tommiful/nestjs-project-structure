{
  "name": "node-nestjs-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build:entity": "rimraf dist/entity && nest build -p ./src/entity/tsconfig.json",
    "build": "npm run lint && rimraf dist && nest build -p ./src/entity/tsconfig.json && nest build",
    "prestart": "npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.6",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "^7.0.6",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.6",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^7.0.0",
    "aws-sdk": "^2.647.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "compression": "^1.7.4",
    "express-session": "^1.17.0",
    "helmet": "^3.22.0",
    "mysql": "^2.18.1",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.0.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "dtslint": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^25.2.3",
    "prettier": "^2.0.2",
    "prompts": "^2.3.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.1",
    "typeorm-model-generator": "^0.4.1-no-engines.2",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-nestjs-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://tested.kr",
  "author": "infiltrator@naver.com",
  "license": "MIT"
}
