{
  "name": "node-nestjs-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build": "rimraf dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "jest --detectOpenHandles",
    "test:e2e": "nest build -p tsconfig.e2e.json && jest --config ./test/jest.e2e.js",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "0.6.1",
    "@nestjs/core": "^7.5.5",
    "@nestjs/graphql": "^7.9.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.5",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.19.0",
    "aws-sdk": "^2.799.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "helmet": "^4.2.0",
    "mysql": "^2.18.1",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.3",
    "@nestjs/testing": "^7.5.5",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "dtslint": "^4.0.6",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prompts": "^2.4.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "typeorm-model-generator": "^0.4.3",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-nestjs-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/node-nestjs-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
