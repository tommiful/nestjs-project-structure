{
  "name": "node-nestjs-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build:entity": "rimraf dist/entity && nest build -p ./src/entity/tsconfig.json",
    "build": "npm run lint && rimraf dist && nest build -p ./src/entity/tsconfig.json && nest build",
    "prestart": "npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "0.0.8",
    "@nestjs/core": "^6.10.14",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/serve-static": "^1.0.2",
    "@nestjs/swagger": "^4.1.2",
    "@nestjs/typeorm": "^6.2.0",
    "aws-sdk": "^2.598.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "mysql": "^2.17.1",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^6.13.1",
    "@types/compression": "^1.0.1",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/passport-local": "^1.0.33",
    "dtslint": "^2.0.5",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typeorm-model-generator": "^0.3.5",
    "typescript": "^3.7.4",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-nestjs-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://tested.kr",
  "author": "infiltrator@naver.com",
  "license": "MIT"
}
