{
  "name": "node-nestjs-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build:entity": "rimraf dist/entity && nest build -p ./src/entity/tsconfig.json",
    "build": "npm run lint && rimraf dist && npm run build:entity && nest build",
    "prestart": "npm run build",
    "start": "node dist/app",
    "start:dev": "nest start --watch",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/common": "^7.2.0",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.2.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "aws-sdk": "^2.707.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "mysql": "^2.18.1",
    "nest-router": "^1.0.9",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.4.1",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "dtslint": "^3.6.12",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "prompts": "^2.3.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.1.1",
    "typeorm-model-generator": "^0.4.2-no-engines",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-nestjs-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/node-nestjs-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
