{
  "name": "node-nestjs-structure",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build": "rimraf dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "apollo-server-express": "^3.5.0",
    "aws-sdk": "^2.1036.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express-session": "^1.17.2",
    "graphql": "^15.6.0",
    "graphql-tools": "^8.2.0",
    "helmet": "^4.6.0",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.30",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.16",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/testing": "^8.2.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typeorm-model-generator": "^0.4.6-no-engines",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/node-nestjs-structure.git"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/CatsMiaow/node-nestjs-structure#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
